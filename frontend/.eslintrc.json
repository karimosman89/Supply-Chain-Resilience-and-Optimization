{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es2020": true,
    "jest": true
  },
  "rules": {
    "no-unused-vars": "off",
    "no-console": "off",
    "eqeqeq": "warn",
    "no-var": "error",
    "prefer-const": "warn",
    "no-undef": "off"
  }
}
